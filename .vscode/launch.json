  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Example",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "cwd": "${workspaceFolder}/example",
      "program": "${workspaceFolder}/example/server.js"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Test",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "cwd": "${workspaceFolder}",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
          "run-script", "test"
      ]
    }
  ]
}


